@techreport{ham_extrinsic_2019,
	type = {preprint},
	title = {Extrinsic noise and heavy-tailed laws in gene expression},
	url = {http://biorxiv.org/lookup/doi/10.1101/623371},
	abstract = {Abstract
          
            Noise in gene expression is one of the hallmarks of life at the molecular scale. Here we derive analytical solutions to a set of models describing the molecular mechanisms underlying transcription of DNA into RNA. Our
            Ansatz
